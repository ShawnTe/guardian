<div id="top-bar">
</div>

<div id="main-content">
  <div>
    <h1>All my events</h1>
    <ul>
      <% @events.each do |event| %>
      <% date = event.created_at %>
      <% formatted_date = date.strftime("%a %B %d") %>
      <li><%= link_to formatted_date,  event_path(event) %></li>
      <%end%>
    </ul>
  </div>
</div>

<div id="bottom-bar">
</div>
